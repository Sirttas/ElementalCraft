{
  "multipart": [
    {
      "apply": {
        "model": "elementalcraft:block/overloadshrine_base"
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "elementalcraft:block/overloadshrine_top"
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "elementalcraft:block/overloadshrine_side"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "elementalcraft:block/overloadshrine_side",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "elementalcraft:block/overloadshrine_side",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "elementalcraft:block/overloadshrine_side",
        "y": 270
